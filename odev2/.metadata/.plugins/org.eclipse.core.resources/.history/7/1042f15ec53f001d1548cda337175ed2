package inheritanceDemo;

public class Main {
	
	public static void main(String[] args) {
		KrediUI krediUI = new KrediUI();
		krediUI.krediHesapla(new OgretmenKrediManager());  //içe yazdığımız new ile bellekte bu örneği oluştur diyoruz
															//OgretmenKrediManager içerisindeki kod burada çalışır
	
	}

}
